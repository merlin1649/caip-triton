{
  "name": "v2",
  "container": {
    "image": "gcr.io/k80-exploration/tritonserver:19.12-py3",
    "args": [
        "trtserver",
        "--model-repository=/model_repository"
    ],
    "env": [
    ], 
    "ports": [
      { "containerPort": 8000 }
    ]
  },
  "routes": {
    "predict": "/api/infer/resnet1/1", 
    "health": "/api/health/ready" 
  },
  "machine_type": "n1-standard-4",
  "acceleratorConfig": {
    "count":1,
    "type":"nvidia-tesla-t4"
  }
}
